[user]
  name='Jason Roth'
  email='jasonmichaelroth@gmail.com'
[core]
  excludesfile = ~/.gitignore
  whitespace = trailing-space,space-before-tab
  quotepath = false
[apply]
  whitespace = warn
[color]
  branch = auto
  diff = auto
  status = auto
  ui = true
[diff]
  mnemonicprefix = true
[alias]
  b = branch
  ru = remote update
  co = checkout
  st = status --short
  l = "!source ~/.githelpers && git_pretty_log"
  dc = diff --cached
  d = diff
  amend = commit --amend
  aa = add --all
[push]
  # Only push branches that have been set up to track a remote branch.
  default = current
